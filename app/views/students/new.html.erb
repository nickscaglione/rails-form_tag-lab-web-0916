<h3>Student Form</h3>

<%= form_tag students_path do %>

  <label>First name:</label><br>
  <input type="text" name="student[first_name]" id="first_name"><br>

  <label>Last name:</label><br>
  <input type="text" name="student[last_name]" id="last_name">

  <%= submit_tag "Submit Student" %>

<% end %>
<%= params.inspect %>
